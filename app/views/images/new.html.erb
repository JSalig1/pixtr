<%= form_for([@gallery, @image]) do |form| %>   <!-- #note the parenthesis are optional and you should leave them off in practice -->
	<%= form.text_field :name, placeholder: "Name" %>
	<%= form.text_field :description, placeholder: "Description" %>
	<%= form.text_field :url, placeholder: "URL" %>
	<%= form.submit "Add Image" %>
<% end %>