<%= form_for :session, url: session_path do |form| %>   <!-- #note the parenthesis are optional and you should leave them off in practice -->
	<div>	
		<%= form.label :email %>
		<%= form.email_field :email %>
	</div>
	<div>
		<%= form.label :password %>
		<%= form.password_field :password %>
	</div>
	<div>
		<%= form.submit "Sign in" %>
	</div>
<% end %>